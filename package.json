{
	"name": "obsidian-clipflow",
	"version": "1.0.0",
	"description": "Seamlessly capture, search, and quick-paste from your clipboard history.",
	"main": "main.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/helalsoft/obsidian-clipflow"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"package": "pnpm run build && bestzip obsidian-clipflow.zip main.js manifest.json styles.css"
	},
	"keywords": [
		"obsidian",
		"clipboard",
		"history"
	],
	"author": "Helalsoft",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.0",
		"bestzip": "^2.2.1",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.19.0",
		"obsidian": "latest",
		"tslib": "^2.6.0",
		"typescript": "^5.3.0"
	}
}
